client.on('message', message => {
if (message.content === prefix+'slow') {
        let letters = "AaBbCcDFfGgHh-MmNnOoP-pQqRrSsT-metube-YT-tUuVvWw-XxYyZz0123456789".split("");
        let spaces = " ".repeat(10);//by : MeTuBe
        let last = "";
        let messages = 0;
 
        setInterval(() => {
            let str = "";
            let num = Math.floor(Math.random() * 10);//by : MeTuBe
            for(let i = 0; i < num; i++) {
                str += letters[Math.floor(Math.random() * letters.length)]; 
 
                if(i === num - 1) {
                    if(last === "") {
                        messages += 1;//by : MeTuBe
                        last = `${str} ${spaces.slice(str.length + 1)}`;
                        return message.channel.send(`${str} ${spaces.slice(str.length + 1)}${messages}`);
                    } else {
                        messages += 1;
                        let spaced = `${str} ${spaces.slice(str.length + 1)}${messages}`;//by : MeTuBe
                        last = spaced;
                        return message.channel.send(spaced);
                    }
                }
            }
        }, 6000);
}
}); 

